def remdup(l):
    s=[]
    for i in l:
        if i not in s:
            s.append(i)
    return(s)

def iseven(n):
    if n%2==0:
        return True
    
def sumsquare(l):
    a=0
    b=0
    for i in l:
        if iseven(i):
            a=a+(i*i)
        else:
            b=b+(i*i)
    return([b,a])

def transpose(l):
    s=[]
    for i in range(len(l[0])):
        k=[]
        for j in range(len(l)):
            k.append(l[j][i])
        s.append(k)
    return(s)



